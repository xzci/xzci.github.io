			<span class="p">}</span>
		<span class="p">}</span>
		<span class="k">return</span> <span class="n">cnt</span><span class="o">&lt;</span><span class="mi">2</span><span class="p">;</span>

	<span class="p">}</span>
<span class="p">};</span>
